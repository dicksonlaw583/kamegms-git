<SCM_Config>
  <SCM_Commands>
    <pre-add></pre-add>
    <add>add</add>
    <post-add></post-add>
    <pre-blame></pre-blame>
    <blame>blame</blame>
    <post-blame></post-blame>
    <pre-changelog></pre-changelog>
    <changelog>show</changelog>
    <post-changelog></post-changelog>
    <pre-checkout></pre-checkout>
    <checkout>clone</checkout>
    <post-checkout></post-checkout>
    <pre-clean></pre-clean>
    <clean>clean</clean>
    <post-clean></post-clean>
    <pre-commit>add</pre-commit>
    <commit>commit</commit>
    <post-commit></post-commit>
    <pre-delete></pre-delete>
    <delete>rm</delete>
    <post-delete></post-delete>
    <pre-info></pre-info>
    <info></info>
    <post-info></post-info>
    <pre-import></pre-import>
    <import>init</import>
    <post-import>postimport</post-import>
    <pre-lock></pre-lock>
    <lock></lock>
    <post-lock></post-lock>
    <pre-rename></pre-rename>
    <rename>mv</rename>
    <post-rename></post-rename>
    <pre-resolve></pre-resolve>
    <resolve>checkout</resolve>
    <post-resolve></post-resolve>
    <pre-resolved></pre-resolved>
    <resolved>add</resolved>
    <post-resolved></post-resolved>
    <pre-revert></pre-revert>
    <revert>checkout</revert>
    <post-revert></post-revert>
    <pre-showlog></pre-showlog>
    <showlog>log</showlog>
    <post-showlog></post-showlog>
    <pre-status></pre-status>
    <status>status</status>
    <post-status></post-status>
    <pre-unlock></pre-unlock>
    <unlock></unlock>
    <post-unlock></post-unlock>
    <pre-update></pre-update>
    <update>pull</update>
    <post-update></post-update>
    <pre-revision-revert></pre-revision-revert>
    <revision-revert>checkout</revision-revert>
    <post-revision-revert></post-revision-revert>
  </SCM_Commands>
  <SCM_Command_Options>
    <pre-add></pre-add>
    <add>$FILE --force</add>
    <post-add></post-add>
    <pre-blame></pre-blame>
    <blame>$FILE</blame>
    <post-blame></post-blame>
    <pre-changelog></pre-changelog>
    <changelog>--name-status --pretty=format:"commit:%H | %an | %ad %n%s%nChanged paths:" $REVISION</changelog>
    <post-changelog></post-changelog>
    <pre-checkout></pre-checkout>
    <checkout>$SERVERURL $PATH</checkout>
    <post-checkout></post-checkout>
    <pre-clean></pre-clean>
    <clean>-f</clean>
    <post-clean></post-clean>
    <pre-commit>$FILE</pre-commit>
    <commit>-m $MESSAGE</commit>
    <post-commit></post-commit>
    <pre-delete></pre-delete>
    <delete>$FILE --force</delete>
    <post-delete></post-delete>
    <pre-info></pre-info>
    <info></info>
    <post-info></post-info>
    <pre-import></pre-import>
    <import>.</import>
    <post-import>$SERVERURL</post-import>
    <pre-lock></pre-lock>
    <lock></lock>
    <post-lock></post-lock>
    <pre-rename></pre-rename>
    <rename>$FILE $RENAMED</rename>
    <post-rename></post-rename>
    <pre-resolve></pre-resolve>
    <resolve>--ours $FILE</resolve>
    <resolve-mine>--ours $FILE</resolve-mine>
    <resolve-theirs>--theirs $FILE</resolve-theirs>
    <post-resolve></post-resolve>
    <pre-resolved></pre-resolved>
    <resolved>$FILE</resolved>
    <post-resolved></post-resolved>
    <pre-revert></pre-revert>
    <revert>$FILE</revert>
    <post-revert></post-revert>
    <pre-showlog></pre-showlog>
    <showlog>--pretty=format:"commit:%H | %an | %ad | %f" --name-status</showlog>
    <post-showlog></post-showlog>
    <pre-status></pre-status>
    <status>$FILE --porcelain</status>
    <post-status></post-status>
    <pre-unlock></pre-unlock>
    <unlock></unlock>
    <post-unlock></post-unlock>
    <pre-update></pre-update>
    <update>origin master</update>
    <post-update></post-update>
    <status-all>$FILE --porcelain</status-all>
    <pre-revision-revert></pre-revision-revert>
    <revision-revert>$REVISION -- $FILE</revision-revert>
    <post-revision-revert></post-revision-revert>
  </SCM_Command_Options>
  <SCM_External_Tools>
    <mergetool>TortoiseGitProc</mergetool>
  </SCM_External_Tools>
  <SCM_External_Tools_Options>
    <mergetool>/command:conflicteditor /path:$merged</mergetool>
  </SCM_External_Tools_Options>
  <SCM_StatusLog_Headers>
    <status-from>From Status</status-from>
    <status>To Status</status>
    <filepath>FilePath</filepath>
  </SCM_StatusLog_Headers>
  <SCM_StatusLog_Format>
    <status-from> ,M,A,D,R,C,?,!,U</status-from>
    <status> ,M,A,D,R,C,?,!,U</status>
    <filepath>$PROJECTPATH</filepath>
  </SCM_StatusLog_Format>
  <SCM_StatusLog_Codes>
    <Unmodified></Unmodified>
    <Modified>M</Modified>
    <Added>A</Added>
    <Deleted>D</Deleted>
    <Replaced>R</Replaced>
    <Conflicted>U</Conflicted>
    <Updated>M</Updated>
    <External></External>
    <Ignored>!</Ignored>
    <Unversioned>?</Unversioned>
    <Missing></Missing>
    <Wrong-Type></Wrong-Type>
    <Copied>C</Copied>
    <Clean></Clean>
  </SCM_StatusLog_Codes>
  <SCM_Common_Functions>
    <RefreshAll>Refresh</RefreshAll>
    <AddAll>Add All</AddAll>
    <CommitAll>Commit All</CommitAll>
    <Revert>Revert</Revert>
    <RevertAll>Revert All</RevertAll>
  </SCM_Common_Functions>
  <SCM_Additional_Functions>
    <menu-blame>Blame</menu-blame>
    <menu-diff>Diff</menu-diff>
    <none1>==========</none1>
    <menu-pull>Pull...</menu-pull>
    <menu-push>Push...</menu-push>
    <menu-switch>Switch to...</menu-switch>
    <menu-branch>Branch...</menu-branch>
    <menu-tag>Tag...</menu-tag>
